{
  "tags": {
	"allowUnknownTags": true,
	"dictionaries": ["jsdoc", "closure"]
  },
  "source": {
	"include": ["src"],
	"includePattern": ".+\\.js(doc|x)?$",
	"excludePattern": "(^|\\/|\\\\)_"
  },
  "templates": {
	"better-docs": {
	  "name": "OH-MY-SVG Wordpress Block"
	}
  },
  "opts": {
	"template": "node_modules/clean-jsdoc-theme",
	"destination": "docs",
	"recurse": true,
	"sort": true
  }
}
