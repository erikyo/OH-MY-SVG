{
  "tags": {
	"allowUnknownTags": true,
	"dictionaries": [
	  "jsdoc",
	  "closure"
	]
  },
  "plugins": [
	"plugins/markdown",
	"node_modules/better-docs/typescript"
  ],
  "typescript": {
	"moduleRoot": "src"
  },
  "source": {
	"include": [
	  "src"
	],
	"includePattern": "\\.(jsx|js|ts|tsx)$",
	"excludePattern": "(^|\\/|\\\\)_"
  },
  "templates": {
	"cleverLinks": false,
	"monospaceLinks": false,
	"better-docs": {
	  "name": "OH-MY-SVG Wordpress Block",
	  "title": "OH-MY-SVG",
	  "hideGenerator": true
	}
  },
  "opts": {
	"encoding": "utf8",
	"destination": "docs",
	"template": "./node_modules/better-docs",
	"readme": "./readme.txt",
	"recurse": true
  },
  "markdown": {
	"hardwrap": false,
	"idInHeadings": true
  }
}
