{
	"apiVersion": 2,
	"name": "codekraft/oh-my-svg",
	"title": "SVG",
	"attributes": {
		"svg": {
			"type": "string",
            "source": "html",
            "selector": ".wp-block-codekraft-oh-my-svg"
        },
		"originalSvg": {
			"type": "string",
            "source": "html",
            "selector": ".wp-block-codekraft-oh-my-svg"
        },
		"href": {
			"type": "string"
		},
		"alt": {
			"type": "string"
		},
		"rel": {
			"type": "string"
		},
		"linkTarget": {
			"type": "string",
            "default": "_self"
		},
		"title": {
			"type": "string"
		},
		"height": {
			"type": "number"
		},
		"width": {
			"type": "number"
		},
		"rotation": {
			"type": "number",
			"default": 0
		},
		"style": {
			"type": "object"
		}
	},
	"supports": {
		"align": true,
		"anchor": true,
		"className": true,
		"color": {
			"background": true,
			"gradients": true,
			"text": false,
			"__experimentalDuotone": "svg"
		},
		"filter": {
			"duotone": true
		},
		"__experimentalBorder": {
			"__experimentalSkipSerialization": true,
			"radius": true,
			"width": true,
			"color": true,
			"style": true,
			"__experimentalDefaultControls": {
				"radius": true
			}
		},
        "selectors": {
          "border": "svg",
          "filter": {
            "duotone": "svg"
          }
        },
		"spacing": {
			"margin": true,
			"padding": true,
			"blockGap": true
		},
		"default": {
			"align": "none",
			"display": "table"
		}
	},
	"selectors": {
		"filter": {
			"duotone": ".wp-block-codekraft-oh-my-svg svg"
		}
	},
	"category": "media",
	"editorScript": "file:./build/oh-my-svg-editor.js"
}
