{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "codekraft/oh-my-svg",
	"category": "media",
	"title": "SVG",
	"attributes": {
		"svg": {
			"type": "string",
            "source": "html",
            "selector": ".wp-block-codekraft-oh-my-svg .svg-block-wrapper"
        },
		"href": {
			"type": "string"
		},
		"rel": {
			"type": "string"
		},
		"linkTarget": {
			"type": "string",
            "default": "_self"
		},
		"title": {
			"type": "string"
		},
		"height": {
			"type": "number"
		},
		"filesize": {
			"type": "number"
		},
		"width": {
			"type": "number"
		},
		"aspectRatio": {
			"type": "string"
		},
		"rotation": {
			"type": "number",
			"default": 0
		},
		"style": {
			"type": "object",
			"default": {
				"shadow": "var:preset|shadow|deep"
			}
		}
	},
	"supports": {
		"align": true,
		"ariaLabel": true,
		"anchor": true,
		"className": true,
		"layout": true,
		"filter": {
			"duotone": true,
			"gradients": true,
			"opacity": true,
			"shadow": true
		},
		"color": {
			"background": true,
			"gradients": true,
			"text": false,
			"__experimentalDuotone": "svg"
		},
		"__experimentalBorder": {
			"color": true,
			"radius": true,
			"style": true,
			"width": true,
			"__experimentalSkipSerialization": true,
			"__experimentalDefaultControls": {
				"color": true,
				"radius": true,
				"style": true,
				"width": true
			}
		},
		"spacing": {
			"margin": true,
			"padding": true,
			"blockGap": true
		},
		"dimensions": {
			"aspectRatio": true,
			"minHeight": false
		},
		"position": {
			"sticky": false
		},
		"shadow": {
			"__experimentalSkipSerialization": true
		},
		"default": {
			"align": "none"
		}
	},
	"selectors": {
		"root": ".wp-block-codekraft-oh-my-svg .svg-block-wrapper",
		"border": ".wp-block-codekraft-oh-my-svg .svg-block-wrapper",
		"shadow": ".wp-block-codekraft-oh-my-svg .svg-block-wrapper",
		"typography": ".wp-block-codekraft-oh-my-svg .svg-block-wrapper > svg text",
		"filter": {
			"duotone": ".wp-block-codekraft-oh-my-svg .svg-block-wrapper"
		},
		"color": {
			"text": ".wp-block-codekraft-oh-my-svg .svg-block-wrapper > svg text"
		}
	},
	"editorScript": "file:./index.js"
}
